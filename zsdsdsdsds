(function(win, doc) {
  function loadScript(src, onComplete) {
    var s = doc.createElement('script');
    s.type = 'text/javascript';
    s.async = true;
    s.src = src;

    if (onComplete) {
      s.onreadystatechange = function() {
        if (this.readyState === 'complete' || this.readyState === 'loaded') {
          onComplete();
        }
      };
      s.onload = onComplete;
    }

    var fs = doc.getElementsByTagName('script')[0];
    fs.parentNode.insertBefore(s, fs);
  }

  function loadPixel(ppcSrc) {
    var image = new Image(1,1);
    image.src = ppcSrc;
  }

  
  (function() {
    // in old GA, we did tracking with "gaq.push()"
    // in the new GA, tracking is done via a function in form "gaq(TRACKING_PARAMS)"
    // this function is for pre-load before GA JS is loaded on page
    // similar to doing "var gaq = win['gaq'] || []"
    win['gaq'] = win['gaq'] ||
            function() {
                (win['gaq'].q = win['gaq'].q || []).push(arguments);
            };

    gaq('create', 'UA-18268683-1', 'auto');
    gaq('require', 'linkid', 'linkid.js');

    
    gaq('set', 'dimension1', 'SS');
    
    gaq('set', 'dimension2', 'A');

    gaq('set', 'dimension3', 'C');
    gaq('send', 'pageview');


    //record all pdf clicks in community pages and send them to google analytics.
    Y.use('event-delegate', function () {
      Y.delegate('mousedown', function(e) {
          var pdfUri = e.currentTarget.getAttribute('href');
          // Some times the filter fails, so we want to double check again.
          // And only send to ga if it is an pdf link.
          if (pdfUri && pdfUri.lastIndexOf(".pdf") === pdfUri.length - 4) {
            gaq('send', 'pageview', pdfUri);
            gaq('send', 'event', 'PDF_Click', pdfUri, win.location.pathname);
          }
      }, 'body', 'a[href$=".pdf"]');
    });

    




try {
  window.addEventListener('error', function(e) {
    var msg =
      e.message +
      ' @ ' + window.location.href +
      ':' + e.filename +
      ':' + e.lineno +
      ':' + e.colno +
      ':' + 'true';

    gaq('send', 'exception', {
      'exDescription': msg
    });
    console.log(msg);
  });
}
catch (e) {
  // shouldn't happen, but just in case addEventListener fails
}

    gaq.l = new Date().getTime();
    win['GoogleAnalyticsObject'] = 'gaq';
    loadScript('//www.google-analytics.com/analytics.js');
  })();

  
  

}(window, document));






(function(w, d, s, l, i) {

  w[l] = w[l] || [];
  w[l].push({
    'gtm.start': new Date().getTime(),
    event: 'gtm.js'
  });

  var f = d.getElementsByTagName(s)[0],
  j = d.createElement(s),
  dl = l != 'dataLayer' ? '&l=' + l : '';
  j.async = true;
  j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
  f.parentNode.insertBefore(j, f);
})(window, document, 'script', 'dataLayer', 'GTM-MQKK9F');

// Runs sign-in pixels after a user signs in.










  ;(function () {
    try { 
        var pattern = new RegExp("^(\\d{1,3}\\.){4}\\d{16}$");
        Y.use('cookie', function () {
            
            if (pattern.test(localStorage.getItem("TRID"))) {
                var d = new Date();
                Y.Cookie.set("TRID",
                        localStorage.getItem("TRID"), {
                            path: "/",
                            domain: "ixl.com",
                            expires: d.setFullYear(d.getFullYear() + 1)
                        });
            }
            
            
            
            else if (pattern.test(Y.Cookie.get("TRID"))) {
                localStorage.setItem("TRID",
                        Y.Cookie.get("TRID"));
            }
        });
    } catch(e) {
        return false;
    }
  })();

})();

  </script>
  










  


</body></html>
